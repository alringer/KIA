    <div class="remote-module">
      <div class="overview-remote-commands-header">
        <i class="open-info" data-toggle="modal" data-target="#about-battery">
          i
        </i><!-- /.open-info -->
        <span class="remote-commands-refresh" data-action="refresh-page">
          <span class="messages">
            <span class="last-refreshed">
              Last refreshed
              <span class="time">4 hrs ago</span>
            </span><!-- /.last-refreshed -->
            <span class="refreshing">
              Refreshing Status. Please Wait...
            </span>
          </span><!-- /.messages -->
          <span class="updating-icon-status">
            <span class="refresh-ready active"><i class="icon-refresh"></i></span>
            <span class="refreshing"><i class="icon-communicating"></i></span>
          </span><!-- /.updating-icon-status -->
        </span><!-- /.remote-commands-refresh -->
      </div><!-- /.remote-header -->
      <div class="remote-body">
        <div class="remote-meta-column left">
          <strong>Total Ranges</strong>
          <div class="remote-meta">
            <i class="icon-gas-battery"></i>
            <strong>316 mi</strong>
            <em>Max Combined Range</em>
          </div><!-- /.remote-meta -->
          <div class="remote-meta">
            <i class="icon-battery-small"></i>
            <strong>18 mi</strong>
            <em>Max Battery Range</em>
          </div><!-- /.remote-meta -->
        </div><!-- /.remote-meta -->
        <div class="remote-meta-column right">
          <strong>Estimated Charge Time</strong>
          <div class="remote-meta">
            <i class="icon-clock"></i>
            <strong>1 hr. 30min</strong>
            <em>Until Full charger</em>
          </div><!-- /.remote-meta -->
          <div class="remote-meta">
            <i class="icon-charge-bolt-small"></i>
            <strong>120V AC</strong>
            <em>Level 1 Charging Station</em>
          </div><!-- /.remote-meta -->
        </div><!-- /.remote-meta -->
        <div class="remote-battery">
          <div class="battery percentage-32">
            <span class="percentage">32<sub>%</sub></span>
          </div>
        </div><!-- /.remote-battery -->
      </div><!-- /.remote-body -->
      <div class="remote-footer">
        <div class="overview-remote-command state-1">
          <div class="overview-remote-command-header">
            <div class="overview-remote-command-header-state">
              <span class="state-1 active">
                Battery is charging
              </span><!-- /.current-state -->
              <span class="state-2">
                Charge stopped
              </span>
            </div><!-- /.overview-remote-command-header-state -->
            <div class="transitioning-messages">
              <span class="state-1-2 active">
                Stopping charge...
              </span><!-- /.state-1-2 -->
              <span class="state-2-1">
                Starting charge...
              </span><!-- /.state-2-1 -->
            </div><!-- /.transitioning-messages -->
          </div><!-- /.overview-remote-command-header -->
          <div class="overview-remote-command-icon">
            <span class="icon-remote">
              <i class="icon-charging state-1 active"></i>
              <i class="icon-charging state-2"></i>
            </span><!-- /.icon-remote -->
            <span class="spinner">
              <i class="icon-communicating"></i>
            </span>
          </div><!-- /.overview-remote-command-icon -->
          <div class="overview-remote-command-actions">
            <div ontouchstart="" class="action-buttons split">
              <span ontouchstart="" class="action-button state-1 active" data-action="state-2-1">
                <i class="icon-bolt"></i>
                Start
                <span class="spinner"><i class="icon-communicating white"></i></span>
              </span><!-- /.action-button -->
              <span ontouchstart="" class="action-button state-2" data-action="state-1-2">
                <i class="icon-stop"></i>
                Stop
                <span class="spinner"><i class="icon-communicating white"></i></span>
              </span><!-- /.action-button -->
            </div><!-- /.overview-remtote-command-actions-buttons -->
          </div><!-- /.overview-remote-command-actions -->
        </div><!-- /.overview-remote-command -->
        <div class="remote-footer-caption">
          <strong>
            NOTIFY ME WHEN:
          </strong>
          Begin Charging, Stop Charging, Battery is at 100%
          <i class="icon-cog-white"></i>
        </div><!-- /.remote-footer-caption -->
      </div><!-- /.remote-footer -->
    </div><!-- /.remote-battery -->
    <div class="remote-footer-cards">
      <div class="row">
        <div class="col-sm-6">
