            <a href="#" class="button outline white transparent less-padding">Locate A Station</a>
            <i class="icon-arrow-right white"></i>
          </div><!-- /.remote-footer-card -->
        </div><!-- /.col -->
        <div class="col-sm-6">
          <div class="remote-footer-card charging-etiquette">
