<div class="modal fade add" id="add" tabindex="-1" role="dialog" aria-labelledby="login" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <span class="icon-close" data-dismiss="modal"></span>
      <span class="back" data-target="#add-vehicle-slider" data-slide-to="0"></span>
      <div class="modal-body">
        <div id="add-vehicle-slider" class="carousel slide" data-ride="carousel" data-interval="false">
