            <span class="button" data-dismiss="modal">Yes, Delete</span>
            <span class="button outline" data-dismiss="modal">No</span>
          </div><!-- /.modal-delete-actions -->
        </div><!-- /.modal-delete -->
      </div><!-- /.modal-body -->
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal#delete -->

