                </div><!-- /.modal-find-vin-text -->
              </div><!-- /.modal-find-vi -->
            </div><!-- /.item -->
          </div><!-- /.carousel-inner -->
        </div><!-- /.caorusel -->
      </div><!-- /.modal-body -->
    </div><!-- /.modal-content -->
  </div><!-- /.div class="modal-dialog" -->
</div><!-- /.modal#add -->

<div class="modal fade delete" id="delete" tabindex="-1" role="dialog" aria-labelledby="login" aria-hidden="true">
