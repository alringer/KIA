<div class="has-mobile-nav">
  <!-- @import "partials/nav-dual.kit" -->
  <!-- @import "partials/nav-mobile-remote.kit" -->
  <div class="page-view view-remote">
    <div class="page-view-subnav transparent">
      <ul class="page-view-subnav-items">
        <li>
          <a href="#" class="active">
            Security
          </a>
        </li>
        <li>
          <a href="#">
            Climate
          </a>
        </li>
        <li>
          <a href="#">
            Battery
          </a>
        </li>
      </ul><!-- /.page-view-subnav-items -->
    </div><!-- /.page-view-subnav -->
    <div class="remote-module remote-security">
      <div class="overview-remote-commands-header">
        <i class="open-info" data-toggle="modal" data-target="#about-security">
          i
        </i><!-- /.open-info -->
        <span class="remote-commands-refresh" data-action="refresh-page">
          <span class="messages">
            <span class="last-refreshed">
              Last refreshed
              <span class="time">4 hrs ago</span>
            </span><!-- /.last-refreshed -->
            <span class="refreshing">
              Refreshing Status. Please Wait...
            </span>
          </span><!-- /.messages -->
          <span class="updating-icon-status">
            <span class="refresh-ready active"><i class="icon-refresh"></i></span>
            <span class="refreshing"><i class="icon-communicating"></i></span>
          </span><!-- /.updating-icon-status -->
        </span><!-- /.remote-commands-refresh -->
      </div><!-- /.remote-header -->
      <div class="remote-body">
          <div class="remote-vehicle">
              <div class="remote-vehicle-base open-both">
                  <!--
                      .remote-vehicle-base additional class options:
                      open-both,
                      open-front-only,
                      open-back-only
                   -->
                 <img class="all-doors-closed" src="/images/remote-vehicle/img_optima_ground_all_closed.png" alt="">
                 <img class="open-both-doors" src="/images/remote-vehicle/img_optima_ground_both_open.png" alt="">
                 <img class="open-front-door" src="/images/remote-vehicle/img_optima_ground_front_open.png" alt="">
                 <img class="open-back-door" src="/images/remote-vehicle/img_optima_ground_rear_open.png" alt="">
              </div>
              <div class="remote-vehicle-options-layer">
                  <div class="remote-vehicle-top">
                      <!--
                          each subsection can have an additional class of:
                          opened
                      -->
                      <div class="vehicle-front-passenger">
                          <img class="closed-view" src="/images/remote-vehicle/img_optima_right_front_closed.png" alt="">
                          <img class="opened-view" src="/images/remote-vehicle/img_optima_right_front_open.png" alt="">
                      </div><!-- /.vehicle-front-passenger-->
                      <div class="vehicle-back-passenger">
                          <img class="closed-view" src="/images/remote-vehicle/img_optima_right_rear_closed.png" alt="">
                          <img class="opened-view" src="/images/remote-vehicle/img_optima_right_rear_open.png" alt="">
                      </div><!-- /.vehicle-back-passenger -->

                  </div><!-- /.remote-vehicle-top -->
              </div><!-- /.remote-vehicle-options-layer -->

              <div class="remote-vehicle-body">
                  <img src="/images/remote-vehicle/img_optima_base.png" alt="">
              </div><!-- /.remote-vehicle-body -->

              <div class="remote-vehicle-options-layer">
                  <div class="remote-vehicle-top">
                      <!--
                          each subsection can have an additional class of:
                          opened
                      -->
                      <div class="vehicle-hood">
                          <img class="closed-view" src="/images/remote-vehicle/img_optima_hood_closed.png" alt="">
                          <img class="opened-view" src="/images/remote-vehicle/img_optima_hood_open.png" alt="">
                      </div><!-- /.vehicle-hood -->
                      <div class="vehicle-front-driver opened">
                          <img class="closed-view" src="/images/remote-vehicle/img_optima_left_front_closed.png" alt="">
                          <img class="opened-view" src="/images/remote-vehicle/img_optima_left_front_open.png" alt="">
                      </div><!-- /.vehicle-front-driver -->
                      <div class="vehicle-back-diver opened">
                          <img class="closed-view" src="/images/remote-vehicle/img_optima_left_rear_closed.png" alt="">
                          <img class="opened-view" src="/images/remote-vehicle/img_optima_left_rear_open.png" alt="">
                      </div><!-- /.vehicle-back-diver -->
                      <div class="vehicle-trunk">
                          <img class="closed-view" src="/images/remote-vehicle/img_optima_trunk_closed.png" alt="">
                          <img class="opened-view" src="/images/remote-vehicle/img_optima_trunk_open.png" alt="">
                      </div><!-- /.vehicle-trunk -->

                  </div><!-- /.remote-vehicle-top -->
              </div><!-- /.remote-vehicle-options-layer -->
          </div><!--/.remote-vehicle  -->
      </div><!-- /.remote-body -->
      <div class="remote-footer">
        <div class="overview-remote-command remote-command-security state-1">
          <div class="overview-remote-command-header">
            <div class="overview-remote-command-header-state">
              <span class="state-1 active">
                Vehicle is unlocked
              </span><!-- /.current-state -->
              <span class="state-2">
                Vehicle is locked
              </span>
            </div><!-- /.overview-remote-command-header-state -->
            <div class="transitioning-messages">
              <span class="state-1-2 active">
                Locking doors...
              </span><!-- /.state-1-2 -->
              <span class="state-2-1">
                Unlocking doors...
              </span><!-- /.state-2-1 -->
              <span class="state-x-3">
                Activating horn & lights...
              </span><!-- /.state-2-1 -->
            </div><!-- /.transitioning-messages -->
          </div><!-- /.overview-remote-command-header -->
          <div class="overview-remote-command-icon">
            <span class="icon-remote">
              <i class="icon-lock-dark state-1 active"></i>
              <i class="icon-lock-dark state-2"></i>
            </span><!-- /.icon-remote -->
            <span class="spinner">
              <i class="icon-communicating"></i>
            </span>
          </div><!-- /.overview-remote-command-icon -->
          <div class="overview-remote-command-actions">
            <div ontouchstart="" class="action-buttons split">
              <span ontouchstart="" class="action-button state-1 active" data-action="state-2-1">
                <i class="icon-unlock-white"></i>
                Unlock
                <span class="spinner"><i class="icon-communicating white"></i></span>
              </span><!-- /.action-button -->
              <span ontouchstart="" class="action-button state-2 trigger-remote-error" data-action="state-1-2">
                <i class="icon-lock-white"></i>
                Lock
                <span class="spinner"><i class="icon-communicating white"></i></span>
              </span><!-- /.action-button -->
              <span ontouchstart="" class="action-button confirm-action state-3 show-warning" data-action="state-x-3" data-toggle="modal" data-target="#command-warning">
                <i class="icon-horn"></i>
                <span class="spinner"><i class="icon-communicating white"></i></span>
              </span><!-- /.action-button -->
            </div><!-- /.overview-remtote-command-actions-buttons -->
          </div><!-- /.overview-remote-command-actions -->
          <div class="modal fade command-warning" id="command-warning" tabindex="-1" role="dialog" aria-labelledby="warning" aria-hidden="true">
            <div class="modal-dialog" role="document">
              <div class="modal-content">
                <div class="modal-body">
                  <div class="modal-command-warning text-left">
                    <strong class="modal-title">Horn &amp; Lights</strong>
                    <p>Your vehicleâ€™s horn will sound and the headlights will flash</p>
                    <p>Tip: You can stop Horn & Lights by:</p>
                    <ul>
                      <li>Pressing the <strong>Horn & Lights</strong> button</li>
	                    <li>Pressing the <strong>Lock</strong> or <strong>Unlock</strong> buttons</li>
                    </ul>
                    <div class="modal-actions">
                      <span class="button remote-command-action-button" data-action="state-x-3" data-dismiss="modal">Continue</span>
                      <span class="button outline" data-dismiss="modal">Cancel</span>
                    </div><!-- /.modal-actions -->
                  </div><!-- /.modal-command-warning -->
                </div><!-- /.modal-body -->
              </div><!-- /.modal-content -->
            </div><!-- /.modal-dialog -->
          </div><!-- /.modal#command-warning -->
        </div><!-- /.overview-remote-command -->
        <div class="remote-footer-caption">
          <strong>
            NOTIFY ME WHEN:
          </strong>
          Vehicle is locked, vehicle is unlocked
          <i class="icon-cog-white"></i>
        </div><!-- /.remote-footer-caption -->
      </div><!-- /.remote-footer -->
    </div><!-- /.remote-battery -->
  </div><!-- /.view-securiry -->
</div><!-- /.has-mobile-view -->
