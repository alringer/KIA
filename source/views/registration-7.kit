<!-- @import "partials/header.kit" -->
<!-- @import "partials/nav.kit" -->
<div class="view-registration view-registration-7">
  <div class="contain">
    <div class="view-registration-header">
      <h2>
        Congrats, you’ve just added a 2017 Niro
      </h2>
      <h1>
        Let’s get you set up
      </h1>
      <p>
        Congrats on adding a car to your account! You can now view owner’s manuals, watch videos, upload docs, and more! But thats not all. Your vehicle is UVO-equipped! Learn how to activate it below:
      </p>
    </div><!-- /.view-registration-header -->
    <div class="view-registration-blocks">
      <div class="registration-block large registration-block-confirmation with-car-header">
        <div class="registration-block-inner">
          <div class="registration-block-car-header">
            <img src="/images/image_car_niro.png" alt="Picture of 2017 Niro" title="2017 Niro">
            <strong>
              2017 Niro
            </strong>
          </div><!-- /.registration-block-car-header -->
          <div class="registration-setup-steps">
            <div class="row">
              <div class="col-sm-4">
                <div class="registration-setup-step">
                  <img src="/images/image_registration_step_uvo.jpg" alt="UVO" title="UVO" />
                  <strong>
                    Download UVO Eco
                  </strong>
                  <div class="buttons">
                    <a href="" class="button alt store">
                      <i class="fa fa-apple"></i>
                      App Store
                    </a>
                    <a href="" class="button alt store">
                      <i class="icon-google-play"></i>
                      Google Play
                    </a>
                  </div><!-- /.buttons -->
                  <p>
                    Available on most Android and iPhones
                  </p>
                </div><!-- /.registration-setup-step -->
              </div><!-- /.col -->
              <div class="col-sm-4">
                <div class="registration-setup-step">
                  <img src="/images/image_registration_step_dashboard.jpg" alt="Dashboard" title="Dashboard" />
                  <strong>
                    Step inside your vehicle
                  </strong>
                  <p>
                    Press […], […], and sign in with your credentials
                  </p>
                </div><!-- /.registration-setup-step -->
              </div><!-- /.col -->
              <div class="col-sm-4">
                <div class="registration-setup-step">
                  <img src="/images/image_registration_step_phone.jpg" alt="Dashboard" title="Dashboard" />
                  <strong>
                    Enjoy!
                  </strong>
                  <p>
                    Sign into the app and try out a remote command like cool the cabin to 72 degrees
                  </p>
                </div><!-- /.registration-setup-step -->
              </div><!-- /.col -->
            </div><!-- /.row -->
          </div><!-- /.registration-setup-steps -->
          <div class="form-footer-note">
            Changes are saved automatically. You may log out and resume at any time
          </div><!-- /.form-footer-note -->
        </div><!-- /.reistration-block-inner -->
      </div><!-- /.reistration-block -->
    </div><!-- /.view-registration-blocks -->
    <div class="registation-next-step">
      <a href="#">
        Skip this and go to dashboard
      </a>
    </div><!-- /.registration-next-step -->
  </div><!-- /.contain -->
</div><!-- /.registration -->
<!-- @import "partials/footer.kit" -->
